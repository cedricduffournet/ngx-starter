{
  "name": "ngx-starter",
  "version": "0.0.0",
  "description": "Angular starter",
  "license": "MIT",
  "author": "Cedric DUFFOURNET",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "test": "yarn jest:test",
    "test:watch": "yarn jest:test:watch",
    "test:coverage": "yarn jest:coverage",
    "lint": "ng lint",
    "build": "ng build --prod",
    "build:stat": "ng build --prod --stats-json",
    "analyse": "webpack-bundle-analyzer dist/stats-es2015.json",
    "prettier:check": "prettier \"src/**/*{.ts,.js,.json,.html}\"",
    "prettier:write": "yarn prettier:check --write",
    "jest:clearcache": "jest --clearCache",
    "jest:test": "jest",
    "jest:test:watch": "jest --watch",
    "jest:snapshot": "jest -u",
    "jest:coverage": "jest --coverage",
    "clean:dist": "./node_modules/.bin/rimraf dist",
    "clean:package": "./node_modules/.bin/rimraf node_modules",
    "clean:all": "yarn clean:dist && yarn clean:package"
  },
  "private": true,
  "engines": {
    "node": ">=10.9.0 <=11.12.0",
    "npm": ">=5.3.0",
    "yarn": ">=1.9.2 <2.0.0"
  },
  "dependencies": {
    "@angular/animations": "^8.1.1",
    "@angular/common": "^8.1.1",
    "@angular/compiler": "^8.1.1",
    "@angular/core": "^8.1.1",
    "@angular/forms": "^8.1.1",
    "@angular/platform-browser": "^8.1.1",
    "@angular/platform-browser-dynamic": "^8.1.1",
    "@angular/router": "^8.1.1",
    "@angular/service-worker": "^8.1.1",
    "@fortawesome/angular-fontawesome": "^0.4.0",
    "@fortawesome/fontawesome-free": "^5.9.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.8",
    "@fortawesome/free-brands-svg-icons": "^5.5.0",
    "@fortawesome/free-regular-svg-icons": "^5.5.0",
    "@fortawesome/free-solid-svg-icons": "^5.5.0",
    "@ngrx/effects": "^8.1.0",
    "@ngrx/router-store": "^8.1.0",
    "@ngrx/store": "^8.1.0",
    "@ngrx/store-devtools": "^8.1.0",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "angular2-toaster": "^8.0.0",
    "bootstrap-scss": "4.3.1",
    "codecov": "^3.8.3",
    "core-js": "^2.5.4",
    "ngx-bootstrap": "^3.3.0",
    "normalizr": "^3.4.0",
    "rxjs": "^6.5.2",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.800.6",
    "@angular/cli": "^8.1.1",
    "@angular/compiler-cli": "^8.1.1",
    "@cedricduff/schematics-ngx-starter": "^2.0.0",
    "@types/jest": "^24.0.15",
    "@types/node": "^7.10.6",
    "codelyzer": "~5.0.0",
    "jasmine-marbles": "^0.5.0",
    "jest": "^24.8.0",
    "jest-preset-angular": "^7.1.1",
    "jest-zone-patch": "^0.0.10",
    "jsdom": "^14.1.0",
    "prettier": "^1.18.2",
    "protractor": "~5.1.0",
    "ts-node": "^5.0.1",
    "tslint": "^5.18.0",
    "typescript": "~3.4.5",
    "webpack-bundle-analyzer": "^3.3.2"
  }
}
